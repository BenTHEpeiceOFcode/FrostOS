--[[ API Manager System File ]]--
--[[ This gets loaded on startup! DO NOT EDIT THIS FILE ]]--
local log = "/.frostos/logs/apiman.log"
os.loadAPI("/.frostos/apis/logger")
local FileList = fs.list("/.frostos/apis")

for _, file in ipairs(FileList) do
  os.loadAPI("/.frostos/apis/" .. file)
  logger.info(log, "APIMan: Loaded API: ".. file)
end
